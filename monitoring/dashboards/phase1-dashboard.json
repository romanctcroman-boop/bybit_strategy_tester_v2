        "id": 8,
        "title": "Sensitive Data Filtered",
        "type": "stat",
        "targets": [
          {
            "expr": "sensitive_data_filtered_total",
            "legendFormat": "Filtered Items"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 9,
        "title": "Worker Scaling Events",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(worker_scaling_events_total[5m])",
            "legendFormat": "Scaling Events"
          }
