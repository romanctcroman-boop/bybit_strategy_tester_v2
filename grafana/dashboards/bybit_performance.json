{
  "dashboard": {
    "title": "Bybit Strategy Tester - Performance",
    "tags": ["bybit", "trading", "performance"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(bybit_api_requests_total[5m])",
            "legendFormat": "{{symbol}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"}
        ]
      },
      {
        "id": 2,
        "title": "API Latency (95th percentile)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(bybit_api_duration_seconds_bucket[5m]))",
            "legendFormat": "{{symbol}} - {{interval}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"}
        ]
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(bybit_cache_operations_total{result=\"hit\"}[5m]) / (rate(bybit_cache_operations_total{result=\"hit\"}[5m]) + rate(bybit_cache_operations_total{result=\"miss\"}[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 50, "color": "yellow"},
              {"value": 80, "color": "green"}
            ]
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "rate(bybit_errors_total[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "errps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.1, "color": "yellow"},
              {"value": 1, "color": "red"}
            ]
          }
        }
      },
      {
        "id": 5,
        "title": "Rate Limit Hits",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "increase(bybit_rate_limit_hits_total[1h])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      },
      {
        "id": 6,
        "title": "Cache Size",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "bybit_cache_size_bytes{cache_type=\"redis\"}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "unit": "bytes"
        }
      },
      {
        "id": 7,
        "title": "Candles Fetched (by source)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(bybit_candles_fetched_total[5m])",
            "legendFormat": "{{symbol}} - {{source}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "cps", "label": "Candles/sec"}
        ]
      },
      {
        "id": 8,
        "title": "Historical Fetch Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "bybit_historical_fetch_duration_seconds",
            "legendFormat": "{{symbol}} - {{interval}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"}
        ]
      }
    ]
  }
}
